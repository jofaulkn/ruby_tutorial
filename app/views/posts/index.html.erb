<h1>Posts:</h1>
<% @posts.each do |post|%>
<p>
<%= post.title %><br>
<%= post.body %>
<%= link_to 'read', post %>
<%= link_to 'edit', edit_post_path(post) %>
<%= link_to 'delete', post_path(post), method: :delete, data: {confirm: "Are you sure?"} %>
</p>
<% end %>

<%= link_to "New Post", new_post_path %>
